{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/socketio",
      "dest": "server.ts"
    },
    {
      "src": "/api/socketio/(.*)",
      "dest": "server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "package.json"
    }
  ]
}
